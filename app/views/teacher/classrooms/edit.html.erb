
<%= form_for @classroom, url: teacher_course_classroom_path  do |f| %>
<div class="col-md-4 col-sm-offset-4 well">
<div class="col-md-12">
  <div class="form-group">
    <label for="exampleInputName2">Name classroom</label>
    <%= f.text_field :name, class: "form-control", placeholder: "Name classroom" %>
  </div>
  <div class="form-group">
    <label for="exampleInputEmail2">Subject</label>
    <%= f.text_field :subject, class: "form-control", placeholder: "Subject" %>
  </div>
    <div class="form-group">
      <label for="exampleInputEmail2">Number month</label>
      <%= f.number_field :month, class: "form-control", placeholder: "Number month" %>
    </div>
    <div class="form-group">
      <label for="exampleInputEmail2">Schedule</label>
      <%= f.text_field :schedule, class: "form-control", placeholder: "Schedule" %>
    </div>
  <div class="form-group">
    <label for="exampleInputName2">Date start</label>
    <%= f.text_field :start, type: :date, class: "form-control", placeholder: "Date start" %>
  </div>
  <div class="form-group">
    <label for="exampleInputEmail2">Number</label>
    <%= f.number_field :total, class: "form-control", placeholder: "Number" %>
  </div>
  <div class="form-group">
    <label for="exampleInputEmail2">Fee</label>
    <%= f.text_field :fee, class: "form-control", placeholder: "Fee" %>
  </div>
    <button type="submit" name="button" class="btn btn-primary col-md-12">Edit</button>
  </div>

</div>
<% end %>
